{
	"name": "drop-note",
	"productName": "Drop Note",
	"description": "Note taking app",
	"version": "0.1.0",
	"private": true,
	"main": "./out/app.js",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"compile": "tsc",
		"launch": "electron .",
		"main": "npm run compile && npm run launch",
		"start": "npm run compile && npm run build && electron .",
		"build:dev": "electron-builder build --win --publish never",
		"build:deploy": "electron-builder build --win --publish always"
	},
	"dependencies": {
		"@electron/remote": "^2.0.4",
		"core-js": "^3.6.5",
		"simplemde": "^1.11.2",
		"vue": "^3.0.0"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-typescript": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"@vue/compiler-sfc": "^3.0.0",
		"electron": "^16.0.7",
		"electron-builder": "^22.14.13",
		"typescript": "~4.1.5"
	}
}
